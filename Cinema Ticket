day = str(input())

ticket_price = 0

if day == 'Monday':
    ticket_price = 12

elif day == 'Tuesday':
    ticket_price = 12

elif day == 'Wednesday':
    ticket_price = 14

elif day == 'Thursday':
    ticket_price = 14

elif day == 'Friday':
    ticket_price = 12

elif day == 'Saturday':
    ticket_price = 16

elif day == 'Sunday':
    ticket_price = 16

print(ticket_price)
