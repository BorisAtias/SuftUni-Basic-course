product = str(input())


if product == 'banana':
    print('fruit')
elif product == 'apple':
    print('fruit')
elif product == 'kiwi':
    print('fruit')
elif product == 'cherry':
    print('fruit')
elif product == 'lemon':
    print('fruit')
elif product == 'grapes':
    print('fruit')
elif product == 'tomato':
    print('vegetable')
elif product == 'cucumber':
    print('vegetable')
elif product == 'pepper':
    print('vegetable')
elif product == 'carrot':
    print('vegetable')
else:
    print('unknown')
