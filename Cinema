projection_type = str(input())
row_count = int(input())
columns_count = int(input())

income = 0

if projection_type == 'Premiere':
    income = (row_count * columns_count) * 12.00

if projection_type == 'Normal':
    income = (row_count * columns_count) * 7.50

if projection_type == 'Discount':
    income = (row_count * columns_count) * 5.00


print(f'{income:.2f} leva')
